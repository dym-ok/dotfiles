# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/dym/mambaforge/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/dym/mambaforge/etc/profile.d/conda.sh" ]; then
        . "/Users/dym/mambaforge/etc/profile.d/conda.sh"
    else
        export PATH="/Users/dym/mambaforge/bin:$PATH"
    fi
fi
unset __conda_setup

if [ -f "/Users/dym/mambaforge/etc/profile.d/mamba.sh" ]; then
    . "/Users/dym/mambaforge/etc/profile.d/mamba.sh"
fi
